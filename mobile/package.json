{
  "name": "realsingles-mobile",
  "main": "expo-router/entry",
  "version": "4.0.0",
  "description": "RealSingles - Dating App (React Native/Expo)",
  "expo": {
    "autolinking": {
      "exclude": [
        "react-native-worklets-core"
      ]
    },
    "install": {
      "exclude": [
        "@react-navigation/bottom-tabs",
        "@react-navigation/native",
        "@types/react",
        "@react-native-picker/picker"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-full-screen-notification-incoming-call",
          "react-native-agora-chat"
        ]
      }
    }
  },
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "preflight": "node ./scripts/preflight.js",
    "preflight:android": "node ./scripts/preflight.js --android",
    "preflight:ios": "node ./scripts/preflight.js --ios",
    "dev:android": "pnpm preflight:android && expo run:android",
    "dev:ios": "pnpm preflight:ios && expo run:ios"
  },
  "dependencies": {
